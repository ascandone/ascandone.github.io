    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-transformer-remark": "^3.1.0",
    "gentype": "^3.44.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
