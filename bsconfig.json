{
	"name": "portfolio",
	"bsc-flags": [
		"-bs-super-errors"
	],
	"sources": [
		{
			"dir": "src",
			"subdirs": true
		}
	],
	"gentypeconfig": {
		"language": "typescript",
		"shims": {
			"ReasonReact": "ReactShim"
		},
		"debug": {
			"all": false,
			"basic": false
		}
	},
	"package-specs": [
		{
			"module": "es6",
			"in-source": true
		}
	],
	"suffix": ".bs.js",
	"namespace": true,
	"reason": {
		"react-jsx": 3
	},
	"bs-dependencies": [
		"@rescript/react",
		"@glennsl/bs-json"
	],
	"ppx-flags": []
}
